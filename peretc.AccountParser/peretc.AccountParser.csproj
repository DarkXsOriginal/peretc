<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="RequestHeaders.bulk.txt" />
    <None Remove="RequestParams.json" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="RequestHeaders.bulk.txt" />
    <EmbeddedResource Include="RequestParams.json" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="ParseBody.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>ParseBody.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="ParseBody.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>ParseBody.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>
	

</Project>
